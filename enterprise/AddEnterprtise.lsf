MODULE AddEnterprtise;

REQUIRE Payment, Expense;

onAddCurrency 'Добавление кода валюты' () {
    LOCAL ob = Currency();
    ob() <- GROUP MAX (Currency o) IF sid(o) = 'RUB';
    FOR (Payment p IS Payment) AND NOT currency(p) DO currency(p) <- ob();
    FOR (Expense e IS Expense) AND NOT currency(e) DO currency(e) <- ob();
}
